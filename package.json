{
  "name": "jo-nestjs-api",
  "version": "0.0.104",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"test/unit/*.ts\"",
    "start": "nest start",
    "start:console": "npm run start -- --watch --entryFile repl",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:dev:jobs-processor": "nest start --watch --sourceRoot apps/jobs-processor-app --entryFile main",
    "start:prod-web-app": "node dist/apps/web-app/main",
    "start:prod-jobs-processor-app": "node dist/apps/jobs-processor-app/main",
    "start:local": "concurrently \"node dist/apps/web-app/main\" \"node dist/apps/jobs-processor-app/main\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=3 --showSeed",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node -e \"if (!process.argv[1]) { console.error('Error: Please specify a path!'); process.exit(1); } else { require('child_process').execSync('jest --config ./test/e2e/jest-e2e.json --runInBand ' + process.argv[1], { stdio: 'inherit' }); }\"",
    "test:e2e:all": "jest ./test/e2e/web-app --config ./test/e2e/jest-e2e.json --runInBand --bail && jest ./test/e2e/jobs-processor-app --config ./test/e2e/jest-e2e.json --runInBand --bail",
    "test:integration": "jest --config ./test/integration/jest-integration.json --runInBand",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.650.0",
    "@aws-sdk/client-s3": "^3.715.0",
    "@bull-board/api": "^5.21.4",
    "@bull-board/express": "^5.21.4",
    "@bull-board/nestjs": "^5.21.4",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/faker": "^6.6.11",
    "aws-cdk-lib": "^2.79.1",
    "axios": "^1.4.0",
    "bullmq": "^5.12.12",
    "cache-manager": "^5.5.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "email-validator-net": "^2.1.0",
    "entities": "^6.0.0",
    "express-basic-auth": "^1.2.1",
    "intercom-client": "^5.0.0",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mysql2": "^3.9.7",
    "nestjs-pino": "^3.3.0",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
    "pino-http": "^8.4.0",
    "pino-pretty": "^10.2.0",
    "redis": "^4.6.14",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sanitize-html": "^2.14.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.0",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.17.3",
    "@types/passport": "^1.0.16",
    "@types/passport-http": "^0.3.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "auto-changelog": "^2.5.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "faker": "^6.6.6",
    "jest": "^29.3.1",
    "node-mocks-http": "^1.13.0",
    "prettier": "^2.3.2",
    "redis-memory-server": "^0.11.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test/unit",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/../../dotenv-config.js",
      "<rootDir>/../support/faker.wrapper.ts"
    ]
  }
}
